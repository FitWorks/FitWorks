<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    tools:ignore="UnusedNavigation"
    app:startDestination="@id/homeScreen2">
    <fragment
        android:id="@+id/homeScreen2"
        android:name="com.x.fitworks.HomeScreen"
        android:label="HomeScreen"
        tools:layout="@layout/fragment_home_screen">
        <action
            android:id="@+id/action_homeScreen2_to_registrationScreen"
            app:destination="@id/registrationScreen" />
    </fragment>
    <fragment
        android:id="@+id/registrationScreen"
        android:name="com.x.fitworks.RegistrationScreen"
        android:label="RegistrationScreen"
        tools:layout="@layout/fragment_registration_screen"/>
</navigation>

